#include "queue.h"

void push(Queue* queue, char* item) {
    if (!isFull(queue)) {
        queue->queue[queue->queuePointer++] = item;
    }
}

int pop(Queue* queue, char* item) {
    if (!isEmpty(queue)) {
        return queue->queue[--queue->queuePointer];
    }
}

int isEmpty(Queue* queue) {
    return queue->queuePointer == 0;
}

int isFull(Queue* queue) {
    return queue->queuePointer == QLENGTH;
}
