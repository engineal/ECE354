#ifndef HANDSHAKE_H_
#define HANDSHAKE_H_


#define MSG_ACK            0b10101010
#define MSG_NAK            0b11111111
#define MSG_GET_IMAGE      0b00000000
#define MSG_TRANSMIT_IMAGE 0b00000001
#define MSG_IMAGE_PROC_1   0b00000010
#define MSG_IMAGE_PROC_2   0b00000011
#define MSG_IMAGE_PROC_3   0b00000100
#define MSG_IMAGE_PROC_4   0b00000101
#define MSG_IMAGE_PROC_5   0b00000110
#define MSG_IMAGE_PROC_6   0b00000111


#endif /*HANDSHAKE_H_*/
